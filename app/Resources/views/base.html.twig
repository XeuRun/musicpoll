<!DOCTYPE html>
<head>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Алексей Степанков (XeuRun){% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="favicon.ico">
        <meta name="keywords" content="Степанков, Алексей, Алекс">
        <meta name="description" content="Сайт Алексея Степанкова (XeuRun)">
        <meta name="author" content="Алексей Степанков (XeuRun)">
        {% stylesheets filter='cssrewrite'
            '@MainBundle/Resources/public/css/normalize.css'
            '//cdnjs.cloudflare.com/ajax/libs/angularjs-toaster/0.4.9/toaster.min.css'
            '//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css'
            '@MainBundle/Resources/public/css/select.css'
            '@MainBundle/Resources/public/css/main.css'
        output='css/style.css' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
        {% block stylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body ng-app="musicpoll">
        {% block body %}{% endblock %}
        {% javascripts
            '@MainBundle/Resources/public/js/lib/dklab_realplexor.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts
            '//ajax.googleapis.com/ajax/libs/angularjs/1.4.0-beta.5/angular.js'
            '//ajax.googleapis.com/ajax/libs/angularjs/1.4.0-beta.5/angular-sanitize.js'
            '//ajax.googleapis.com/ajax/libs/angularjs/1.4.0-beta.5/angular-animate.js'
            '//cdnjs.cloudflare.com/ajax/libs/angularjs-toaster/0.4.9/toaster.min.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
